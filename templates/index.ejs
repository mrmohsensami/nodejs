<%- include('./partials/head.ejs') %>

    <body>

        <div class="container ">
            <% const name='Page' %>
                <div><%- include('./partials/nav.ejs') %></div>
                <h1>Home <%= name %>
                </h1>
                <hr>


                <% if(blogs.length> 0) { %>
                    <% blogs.forEach(element=> {%>
                        <div class="col-lg-4">

                            <div class="panel panel-default">
                                <div class="panel-heading">

                                    <h3 class="panel-title">
                                        <%= element.title %>
                                    </h3>
                                </div>
                                <div class="panel-body">

                                    <p>
                                        <%= element.snippet %>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <%}) %>
                            <% } else { %>
                                <p>no blogs</p>
                                <% } %>
        </div>

    </body>

    </html>